<Window x:Class="BoardSimulator.Resources.ActivityTimeLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BoardSimulator.Resources"
        mc:Ignorable="d"
        Title="ActivityTimeLineWindow" Height="300" Width="300">
    
    <Grid>
        <ContentControl 
            Margin="20"
            Content="{Binding ActivityTimeLines_VM, UpdateSourceTrigger=PropertyChanged}" />
    </Grid>
</Window>
